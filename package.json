{
  "name": "express-starter",
  "version": "0.0.1",
  "description": "Basic starter for express node apps",
  "main": "app.js",
  "scripts": {
    "start": "nodemon src/app.js --watch src --exec babel-node",
    "clean": "rm -rf dist",
    "babel": "babel src -d dist",
    "build": "npm run clean && npm run babel",
    "serve": "node dist/app.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rpoltorak/express-starter.git"
  },
  "author": "rpoltorak",
  "license": "MIT",
  "homepage": "https://github.com/rpoltorak/express-starter#readme",
  "dependencies": {
    "express": "^4.14.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "nodemon": "^1.11.0"
  }
}
